{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "jsx-a11y/href-no-hash": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": "off",
    "no-param-reassign": "off",
    "no-script-url": "off",
    "react/jsx-filename-extension": "off",
    "react/require-default-props": "off"
  },
  "globals": {
    "Stripe": true
  }
}
